{
    "name": "joebatta/laravel-reuse",
    "version": "0.5.0-beta1",
    "description": "",
    "keywords": [
        "joebatta",
        "laravel-reuse"
    ],
    "homepage": "https://github.com/joebatta/laravel-reuse",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joey de Vries",
            "email": "hi@giobatta.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "laravel/framework": "^10.16"
    },
    "autoload": {
        "psr-4": {
            "Joebatta\\Reuse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joebatta\\Reuse\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joebatta\\Reuse\\LaravelServiceProvider"
            ],
            "aliases": {
                "Reuse": "Joebatta\\Reuse\\ReuseFacade"
            }
        }
    }
}
